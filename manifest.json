{
  "manifest_version": 3,
  "name": "RoVPN",
  "version": "1.0",
  "permissions": [
    "cookies",
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://www.roblox.com/*"
  ]
}