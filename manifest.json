{
  "manifest_version": 3,
  "name": "RoVPN",
  "version": "1.1",
  "permissions": [
    "cookies",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "RoVPN - OFF|ON"
  },
  "host_permissions": [
    "https://*.roblox.com/*"
  ]
}
